{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  },
  "baseUrl": ".",
  "paths": {
    "@/*": ["./src/*"],
    "@app/*": ["./src/app/*"],
    "@public/*": ["./public/*"],
    "@hooks/*": ["./src/hooks/*"],
    "@utils/*": ["./src/utils/*"],
    "@api/*": ["./src/app/api/*"],
    "@styles/*": ["./src/styles/*"],
    "@assets/*": ["./src/assets/*"],
    "@pages/*": ["./src/pages/*"],
    "@types/*": ["./src/@types/*"],
    "@routes/*": ["./src/routes/*"],
    "@configs/*": ["./src/configs/*"],
    "@services/*": ["./src/services/*"],
    "@components": ["./src/components"],
    "@constants/*": ["./src/constants/*"],
    "@components/*": ["./src/components/*"]
  },
  "include": ["vite.config.ts"]
}
